<barcode_parameters>
  <image_parameters>
    <file>GIMP_flipped.png</file>
    <resolution>
      <width>640</width>
      <height>360</height>
    </resolution>
    <!-- The ROI is the area cropped out of the original image.
         OpenCV algorithms will be applied to the ROI. -->
    <!-- Define the ROI as a narrow stripe across all 3 barcodes. -->
    <image_roi>
      <x>125</x>
      <y>240</y>
      <width>220</width>
      <height>30</height>
    </image_roi>
  </image_parameters>
  <gray_parameters>
    <target>150</target>
    <low_threshold>75</low_threshold>
  </gray_parameters>
</barcode_parameters>