<RobotAction>
    <OpMode id="TEST">
        <parameters>
            <lowest_logging_level>v</lowest_logging_level>
            <!-- Not used in FTCAuto but provides a means of overriding the common
                 ROI definition in the image parameters with one that is specific
                 to the current OpMode. -->
            <image_roi>
                <x>585</x>
                <y>260</y>
                <width>200</width>
                <height>130</height>
            </image_roi>
        </parameters>
        <actions>
             <ANALYZE_BARCODE>
                <ocv_image_provider>file</ocv_image_provider>
                <barcode_recognition>
                    <left_within_roi>
                        <x>20</x>
                        <width>70</width>
                        <shipping_hub_level>SHIPPING_HUB_LEVEL_1</shipping_hub_level>
                    </left_within_roi>
                    <right_within_roi>
                        <x>130</x>
                        <width>70</width>
                        <shipping_hub_level>SHIPPING_HUB_LEVEL_2</shipping_hub_level>
                    </right_within_roi>
                    <barcode_element_npos>
                        <shipping_hub_level>SHIPPING_HUB_LEVEL_3</shipping_hub_level>
                    </barcode_element_npos>
                </barcode_recognition>
            </ANALYZE_BARCODE>
        </actions>
    </OpMode>
</RobotAction>
