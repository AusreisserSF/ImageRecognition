<realsense_parameters>
    <hsv_parameters>
        <hue_name>gold</hue_name>
        <hue_low>5</hue_low>
        <hue_high>30</hue_high>
        <saturation_median_target>0</saturation_median_target>
        <saturation_threshold_low>50</saturation_threshold_low>
        <value_median_target>128</value_median_target>
        <value_threshold_low>180</value_threshold_low>
    </hsv_parameters>
    <depth_camera model="D405">
        <color_camera_fov>84.0</color_camera_fov>
        <scale>.0001</scale>
        <distance_filter>1.00</distance_filter>
        <camera_to_robot_center_meters>0.0635</camera_to_robot_center_meters> <!-- 2.5" = 0.0635 meters -->
    </depth_camera>
</realsense_parameters>